Private Sub savePara()
    Dim i As Integer
    For i = 0 To 11
        If (Val(Me.tSteady(i).Text) = 0) Then Me.tSteady(i).Text = "1"
        dataDim.tSteady(Me.rangeChoice.ListIndex, i) = Val(Me.tSteady(i).Text)
        dataDim.aSteady(Me.rangeChoice.ListIndex, i, 0) = Val(Me.aSteady(i).Text) / 100
        dataDim.aSteady(Me.rangeChoice.ListIndex, i, 1) = Val(Me.aSteadyPlus(i).Text) / 100
    Next
End Sub


