Private Sub aSteady_Change(Index As Integer)
    If (Not IsNumeric(Me.aSteady(Index).Text) And Len(Me.aSteady(Index).Text) > 0) Then
        Me.aSteady(Index).Text = Left(Me.aSteady(Index).Text, Len(Me.aSteady(Index).Text) - 1)
        Me.aSteady(Index).SelStart = Len(Me.aSteady(Index).Text)
    End If
End Sub

